class Solution(object):
    def fizzBuzz(self, n):
        stringArray = []
        for i in range(1, n+1, 1):
            if i % 3 == 0 and i % 5 == 0:
                stringArray.append("FizzBuzz")
            elif i % 5 == 0:
                stringArray.append("Buzz")
            elif i % 3 == 0:
                stringArray.append("Fizz")
            else:
                stringArray.append(str(i))
        return stringArray
